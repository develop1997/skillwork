{
  "cli": {
    "version": ">= 0.45.0",
    "requireCommit": true,
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "ios":{
        "simulator": true
      },
      "android":{
        "buildType": "apk"
      }
    },
    "production": {
      "distribution": "internal",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
